#cd ../bench

rm -r -f $HOME/Prj_Sys/Simon_IPs/Simon_96_96/vhd/lib_simon

vlib lib_simon
vmap lib_simon $HOME/Prj_Sys/Simon_IPs/Simon_96_96/vhd/lib_simon

vcom +acc -work lib_simon  $HOME/Prj_Sys/Simon_IPs/Simon_96_96/vhd//const.vhd
vcom +acc -work lib_simon  $HOME/Prj_Sys/Simon_IPs/Simon_96_96/vhd//round.vhd
vcom +acc -work lib_simon  $HOME/Prj_Sys/Simon_IPs/Simon_96_96/vhd//counter.vhd
vcom +acc -work lib_simon  $HOME/Prj_Sys/Simon_IPs/Simon_96_96/vhd//top.vhd

rm -r ./lib_bench_simon
vlib lib_bench_simon
vmap lib_bench_simon ./lib_bench_simon

vcom +acc -work lib_bench_simon  ./bench_top.vhd
